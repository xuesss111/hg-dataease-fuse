{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/config.ts"],
    "exclude": ["src/**/__tests__/**"],
    "compilerOptions": {
      "composite": true,
      "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
      "types": ["element-plus/global"],
      //  ["element-plus/global","@cacp/ui/lib/index"]
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        // "@element-plus/icons-vue": ["node_modules/@cacp/svg-icons"]
      },
      "strictNullChecks": false
    }
  }